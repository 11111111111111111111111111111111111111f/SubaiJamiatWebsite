<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="icon" type="image/x-icon" href="./images/logo-square.png">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-4bw+/aepP/YC94hEpVNVgiZdgIC5+VKNBQNGCHeKRQN+PtmoHDEXuppvnDJzQIu9" crossorigin="anonymous">
    <link rel="stylesheet" href="./css/donate.css">
    <link rel="stylesheet" href="./css/admin.css">
    <title>Majsid Registeration Admin Page || Subai Jamiat Ahle Hadees Mumbai</title>
</head>

<body>

    <%- include('./common/masjid-navbar') %>

        <div class="container">
            <div class="table-container">
                <table class="table table-striped">
                    <tr>
                        <th>
                            userId
                        </th>
                        <th>
                            Date
                        </th>
                        <th>
                            Masjid Name
                        </th>
                        <th>
                            Masjid Address
                        </th>
                        <th>
                            Masjid Location Link
                        </th>
                        <th>
                            Masjid President Name
                        </th>
                        <th>
                            Masjid President Number
                        </th>
                        <th>
                            Masjid Secretarie Name
                        </th>
                        <th>
                            Masjid Secretarie Number
                        </th>
                        <th>
                            Masjid Document
                        </th>
                        <th>
                            status
                        </th>
                        <th>
                            Approve
                        </th>
                        <th>
                            Reject
                        </th>

                    </tr>
                    <% if(MasjidData.length> 0){ %>
                        <% MasjidData.forEach(value=>{ %>

                            <tr>
                                <td class="no-break">
                                    <%= value.userId %>
                                </td>
                                <td class="no-break">
                                    <%= value.date %>
                                </td>
                                <td>
                                    <%= value.MasjidName %>
                                </td>
                                <td>
                                    <%= value.MasjidAddress %>
                                </td>
                                <td>
                                    <%= value.MasjidLocationLink %>
                                </td>
                                <td>
                                    <%= value.MasjidPresidentName %>
                                </td>
                                <td>
                                    <%= value.MasjidPresidentNumber %>
                                </td>
                                <td>
                                    <%= value.MasjidSecretarieName %>
                                </td>
                                <td>
                                    <%= value.MasjidSecretarieNumber %>
                                </td>

                                <!-- <td>
                                    <a class="btn btn-primary btn-sm" target="_blank"
                                        href="./masjidRegisteration/images/<%= value.image %>">
                                        Show Image
                                    </a>
                                </td> -->
                                <td>
                                    <a class="btn btn-primary btn-sm" target="_blank"
                                        href="./masjidRegisteration/documents/<%= value.document %>">
                                        Show Documents
                                    </a>
                                </td>
                                <td>
                                    <%= value.status %>
                                </td>
                                <td>
                                    <button data-id="<%= value._id %>" class="btn btn-success approve-btn">
                                        Approve
                                    </button>
                                </td>
                                <td>
                                    <button data-id="<%= value._id %>" class=" btn btn-danger reject-btn">
                                        Reject
                                    </button>
                                </td>
                            </tr>

                            <% }) %>
                                <% }else{ %>
                                    <tr>
                                        <td colspan="13">
                                            No Data Present
                                        </td>
                                    </tr>
                                    <% } %>
                </table>
                <div class="pagination">

                    <% if(pagination.page> 1){ %>

                        <a href="/masjid-registeration-admin?page=<%= pagination.page - 1 %>">Prev</a>

                        <% }else{ %>
                            <a href="/masjid-registeration-admin" class="inactive">Prev</a>
                            <% } %>

                                <a href="" class="inactive">

                                    <%= pagination.page %> Of <%= pagination.endPage %>

                                </a>



                                <% if(pagination.endPage> pagination.page){ %>
                                    <a href="/masjid-registeration-admin?page=<%= pagination.page + 1 %>">Next</a>
                                    <% }else{ %>
                                        <a href="/masjid-registeration-admin" class="inactive">Next</a>
                                        <% } %>
                </div>
            </div>
        </div>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.bundle.min.js"
            integrity="sha384-HwwvtgBNo3bZJJLYd8oVXjrBZt8cqVSpeBNS5n7C8IVInixGAoxmnlMuBnhbgrkm"
            crossorigin="anonymous"></script>
        <script src="./javascript/masjidRegisteration/approved.js"></script>
        <script src="./javascript/masjidRegisteration/rejected.js"></script>
</body>

</html>